<div class="container">
  <%= render 'shared/show_action_bar', title: 'Show Contact',
             objects_path: contacts_path, edit_object_path: edit_contact_path(@contact) %>

  <div class="container">
            <div class="row">
        <div class="col-sm-4">
          <p>
            <strong>Name:</strong>
          </p>
        </div>
        <div class="col-sm-8">
          <p>
            <%= @contact.name %>
          </p>
        </div>
      </div>
            <div class="row">
        <div class="col-sm-4">
          <p>
            <strong>Email:</strong>
          </p>
        </div>
        <div class="col-sm-8">
          <p>
            <%= @contact.email %>
          </p>
        </div>
      </div>
        </div>
</div>